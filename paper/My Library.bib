@incollection{wiedermannEmergenceSuperTuringComputational2001,
  title = {Emergence of a {{Super}}-{{Turing Computational Potential}} in {{Artificial Living Systems}}},
  booktitle = {Advances in {{Artificial Life}}},
  author = {Wiedermann, Ji{\v r}{\'i} and van Leeuwen, Jan},
  editor = {Kelemen, Jozef and Sos{\'i}k, Petr},
  year = {2001},
