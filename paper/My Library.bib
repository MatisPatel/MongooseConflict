@article{gandonEvolutionaryEpidemiologyDynamics2009,
  title = {Evolutionary {{Epidemiology}} and the {{Dynamics}} of {{Adaptation}}},
  author = {Gandon, Sylvain and Day, Troy},
  year = {2009},
