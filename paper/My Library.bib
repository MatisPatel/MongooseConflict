@article{grafenCalculatingFitness,
  title = {Calculating Fitness in Uncertain Environments},
  author = {Grafen, Alan},
  pages = {48},
  abstract = {The concept of fitness is central in organismal biology, and is used in theoretical papers, in informal reasoning to make predictions in much field and laboratory work, and is measured in some longterm empirical studies. However, there is considerable variation in quite how fitness is defined, and surrogates for fitness are often consciously employed. A recent highly mathematical paper proposed a new definition of fitness, based on Fisher's fundamental theorem of natural selection, but in a wider framework that encompasses asexuality and sexuality; haploidy, haplodiploidy and diploidy; an arbitrary genetic architecture; finite population size; a class-structured population; arbitrary classes that can include continuous variables; environmental stochasticity; and continuing fluctations in the distribution of the population over the classes. This new definition allows a Price Equation and a fundamental theorem to be proved both for one time period alone, and also for the long-run average. The assumptions allow applications to empirical data, and a simulated example is used to explain the new definition of fitness, and the associated concepts of reproductive value and Williams' reproductive value. Calculations of reproductive value are explained, and the role of additional assumptions is argued to be unavoidable and, indeed, desirable. The simulated example is simple in having six classes composed of two sexes by three ages, and an annual census in an annual breeding population. How to choose the class structure and census period in a different application are discussed in terms of the research questions that can be investigated. A precise verbal explanation is offered of both the Price Equations and both the fundamental theorems. The new definition of fitness is suitable for use across organismal biology, and its adoption and application offer the potential for significant conceptual unification.},
  langid = {english},
  keywords = {_tablet},
  file = {C\:\\Users\\defen\\Zotero\\storage\\C57CVX22\\Grafen_Calculating ﬁtness in uncertain environments.pdf}
}

