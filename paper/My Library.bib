@article{allenAssessingLatitudinalGradients2006,
  title = {Assessing Latitudinal Gradients in Speciation Rates and Biodiversity at the Global Scale},
  author = {Allen, Andrew P. and Gillooly, James F.},
  year = {2006},
  month = aug,
  journal = {Ecology Letters},
  volume = {9},
  number = {8},
  pages = {947--954},
  issn = {1461-0248},
  doi = {10.1111/j.1461-0248.2006.00946.x},
  abstract = {The mechanisms responsible for latitudinal biodiversity gradients have fascinated and perplexed biologists since the time of Darwin. Ecological theory has yielded two general classes of mechanisms to account for variation in biodiversity: dispersal\textendash assembly mechanisms that invoke differences in stochastic rates of speciation, extinction and dispersal; and niche\textendash assembly mechanisms that invoke species differences, species interactions and environmental heterogeneity. Distinguishing between these two classes of mechanisms requires explicit consideration of macroevolutionary dynamics. Here, we assess the importance of dispersal\textendash assembly mechanisms in the origin and maintenance of biodiversity using fossil data that encompass 30~million years of macroevolution for three distinct groups of ocean plankton: foraminifera, nannoplankton and radiolaria. Applying new methods of analysis to these fossil data, we show here for the first time that latitudinal biodiversity gradients exhibit strong positive correlations with speciation rates even after explicitly controlling for variation in sampling effort and for increases in habitat area towards the equator. These findings provide compelling evidence that geographical variation in macroevolutionary dynamics is a primary determinant of contemporary biodiversity gradients, as predicted by dispersal\textendash assembly theory.},
  langid = {english},
  keywords = {Latitudinal gradient,metabolic theory of ecology,neutral theory of biodiversity,niche conservatism,Speciation rate,species–area relationship},
  file = {C\:\\Users\\defen\\Zotero\\storage\\T33QF5RP\\Allen and Gillooly - 2006 - Assessing latitudinal gradients in speciation rate.pdf;C\:\\Users\\defen\\Zotero\\storage\\HXEHD5H6\\abstract.html}
}

@article{allenGlobalBiodiversityBiochemical2002,
