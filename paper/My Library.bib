}

@article{allenKineticEffectsTemperature2006,
  title = {Kinetic Effects of Temperature on Rates of Genetic Divergence and Speciation},
  author = {Allen, Andrew P. and Gillooly, James F. and Savage, Van M. and Brown, James H.},
  year = {2006},
  month = jun,
  journal = {Proceedings of the National Academy of Sciences},
