@article{dedreuGroupCooperation2020,
  title = {Group {{Cooperation}}, {{Carrying}}-{{Capacity Stress}}, and {{Intergroup Conflict}}},
  author = {De Dreu, Carsten K. W. and Gross, J{\"o}rg and Fari{\~n}a, Andrea and Ma, Yina},
  year = {2020},
