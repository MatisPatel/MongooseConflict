@article{jedrusikRoleCdx2Cell2008,
  title = {Role of {{Cdx2}} and Cell Polarity in Cell Allocation and Specification of Trophectoderm and Inner Cell Mass in the Mouse Embryo},
  author = {Jedrusik, Agnieszka and Parfitt, David-Emlyn and Guo, Guoji and Skamagki, Maria and Grabarek, Joanna B. and Johnson, Martin H. and Robson, Paul and {Zernicka-Goetz}, Magdalena},
  year = {2008},
