@article{klugOriginParentalCare2013,
  title = {The Origin of Parental Care in Relation to Male and Female Life History},
  author = {Klug, Hope and Bonsall, Michael B. and Alonzo, Suzanne H.},
  year = {2013},
  month = apr,
  journal = {Ecology and Evolution},
  volume = {3},
  number = {4},
  pages = {779--791},
  issn = {2045-7758},
  doi = {10.1002/ece3.493},
  abstract = {The evolution of maternal, paternal, and bi-parental care has been the focus of a great deal of research. Males and females vary in basic life-history characteristics (e.g., stage-specific mortality, maturation) in ways that are unrelated to parental investment. Surprisingly, few studies have examined the effect of this variation in male and female life history on the evolution of care. Here, we use a theoretical approach to determine the sex-specific life-history characteristics that give rise to the origin of paternal, maternal, or bi-parental care from an ancestral state of no care. Females initially invest more into each egg than males. Despite this inherent difference between the sexes, paternal, maternal, and bi-parental care are equally likely when males and females are otherwise similar. Thus, sex differences in initial zygotic investment do not explain the origin of one pattern of care over another. However, sex differences in adult mortality, egg maturation rate, and juvenile survival affect the pattern of care that will be most likely to evolve. Maternal care is more likely if female adult mortality is high, whereas paternal care is more likely if male adult mortality is high. These findings suggest that basic life-history differences between the sexes can alone explain the origin of maternal, paternal, and bi-parental care. As a result, the influence of life-history characteristics should be considered as a baseline scenario in studies examining the origin of care.},
  langid = {english},
  keywords = {_tablet,Biparental care,invasion analysis,life-history,maternal care,parental care,paternal care},
  file = {C\:\\Users\\defen\\Zotero\\storage\\NRT2IQID\\Klug et al_2013_The origin of parental care in relation to male and female life history.pdf;C\:\\Users\\defen\\Zotero\\storage\\GA8R9XX2\\abstract.html}
}

@article{klugSexDifferencesLife2013,
  title = {Sex Differences in Life History Drive Evolutionary Transitions among Maternal, Paternal, and Bi-Parental Care},
  author = {Klug, Hope and Bonsall, Michael B and Alonzo, Suzanne H},
  year = {2013},
  month = apr,
  journal = {Ecology and Evolution},
  volume = {3},
  number = {4},
  pages = {792--806},
  issn = {2045-7758},
  doi = {10.1002/ece3.494},
  abstract = {Evolutionary transitions among maternal, paternal, and bi-parental care have been common in many animal groups. We use a mathematical model to examine the effect of male and female life-history characteristics (stage-specific maturation and mortality) on evolutionary transitions among maternal, paternal, and bi-parental care. When males and females are relatively similar \textendash{} that is, when females initially invest relatively little into eggs and both sexes have similar mortality and maturation \textendash{} transitions among different patterns of care are unlikely to be strongly favored. As males and females become more different, transitions are more likely. If females initially invest heavily into eggs and this reduces their expected future reproductive success, transitions to increased maternal care (paternal \textrightarrow{} maternal, paternal \textrightarrow{} bi-parental, bi-parental \textrightarrow{} maternal) are favored. This effect of anisogamy (i.e., the fact that females initially invest more into each individual zygote than males) might help explain the predominance of maternal care in nature and differs from previous work that found no effect of anisogamy on the origin of different sex-specific patterns of care from an ancestral state of no care. When male mortality is high or male egg maturation rate is low, males have reduced future reproductive potential and transitions to increased paternal care (maternal \textrightarrow{} paternal, bi-parental \textrightarrow{} paternal, maternal \textrightarrow{} bi-parental) are favored. Offspring need (i.e., low offspring survival in the absence of care) also plays a role in transitions to paternal care. In general, basic life-history differences between the sexes can drive evolutionary transitions among different sex-specific patterns of care. The finding that simple life-history differences can alone lead to transitions among maternal and paternal care suggests that the effect of inter-sexual life-history differences should be considered as a baseline scenario when attempting to understand how other factors (mate availability, sex differences in the costs of competing for mates) influence the evolution of parental care.},
  pmcid = {PMC3631395},
  pmid = {23610625},
  keywords = {_tablet},
  file = {C\:\\Users\\defen\\Zotero\\storage\\IW7VW79V\\Klug et al_2013_Sex differences in life history drive evolutionary transitions among maternal,.pdf}
}

