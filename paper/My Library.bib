@article{linlinyuhengResolvingParadox2019,
  title = {Resolving the {{Paradox}} of {{Environmental Quality}} and {{Sociality}}: The {{Ecological Causes}} and {{Consequences}} of {{Cooperative Breeding}} in {{Two Lineages}} of {{Birds}}},
  shorttitle = {Resolving the {{Paradox}} of {{Environmental Quality}} and {{Sociality}}},
  author = {Lin (林宇恆), Yu-Heng and Chan (詹仕凡), Shih-Fan and Rubenstein, Dustin R. and Liu (劉彥廷), Mark and Shen (沈聖峰), Sheng-Feng},
  year = {2019},
  month = aug,
  journal = {The American Naturalist},
  volume = {194},
  number = {2},
  pages = {207--216},
  issn = {0003-0147, 1537-5323},
  doi = {10.1086/704090},
  abstract = {Cooperatively breeding animals occur in virtually every ecosystem on earth. Comparative and biogeographic studies suggest that both benign and harsh\textemdash as well as stable and fluctuating\textemdash environments can favor the evolution of cooperative breeding behavior. The fact that cooperative societies occur in environments of such contrasting quality creates a paradox of environmental quality and sociality. The dual benefits framework\textemdash which leads to the prediction that the ecological consequences of sociality (e.g., range size) vary depending on the benefits that individuals of each species receive by forming social groups\textemdash offers a potential resolution to this paradox. Here we use a case study of two avian lineages, starlings (Sturnidae) and hornbills (Bucerotidae), in which environmental unpredictability appears to have opposite effects on the evolution of cooperation to test the dual benefits framework. Consistent with previous work, harsh and unpredictable environments promote cooperative breeding behavior in starlings, which in turn leads to larger geographic ranges. However, cooperatively breeding hornbills occur in benign and stable environments, but sociality does not influence range size. Our study suggests that the paradox of environmental quality and sociality arises largely because cooperative breeding is an umbrella term encompassing social species that form groups for different reasons. We demonstrate that differentiating among the functional causes of social group formation is critical for developing a predictive framework for understanding the evolution of cooperative breeding behavior.},
  langid = {english},
  file = {C\:\\Users\\defen\\Zotero\\storage\\K6W4LLSA\\Lin (林宇恆) et al. - 2019 - Resolving the Paradox of Environmental Quality and.pdf}
}

