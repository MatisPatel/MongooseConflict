@article{hughesAncestralMonogamyShows2008,
  title = {Ancestral {{Monogamy Shows Kin Selection Is Key}} to the {{Evolution}} of {{Eusociality}}},
  author = {Hughes, William O. H. and Oldroyd, Benjamin P. and Beekman, Madeleine and Ratnieks, Francis L. W.},
  year = {2008},
