@incollection{eckertCellSignallingBlastocyst2015,
  title = {Cell {{Signalling During Blastocyst Morphogenesis}}},
  booktitle = {Cell {{Signaling During Mammalian Early Embryo Development}}},
  author = {Eckert, Judith J. and Velazquez, Miguel A. and Fleming, Tom P.},
  editor = {Leese, Henry J. and Brison, Daniel R.},
  year = {2015},
  series = {Advances in {{Experimental Medicine}} and {{Biology}}},
  number = {843},
  pages = {1--21},
  publisher = {{Springer New York}},
  doi = {10.1007/978-1-4939-2480-6_1},
  abstract = {Blastocyst morphogenesis is prepared for even before fertilisation. Information stored within parental gametes can influence both maternal and embryonic gene expression programmes after egg activation at fertilisation. A complex network of intrinsic, cell-cell mediated and extrinsic, embryo-environment signalling mechanisms operates throughout cleavage, compaction and cavitation. These signalling events not only ensure developmental progression, cell differentiation and lineage allocation to inner cell mass (embryo proper) and trophectoderm (future extraembryonic lineages) but also provide a degree of developmental plasticity ensuring survival in prevailing conditions by adaptive responses. Indeed, many cellular functions including differentiation, metabolism, gene expression and gene expression regulation are subject to plasticity with short- or long-term consequences even into adult life. The interplay between intrinsic and extrinsic signals impacting on blastocyst morphogenesis is becoming clearer. This has been best studied in the mouse which will be the focus of this chapter but translational significance to human and domestic animal embryology will be a focus in future years.},
  copyright = {\textcopyright 2015 Springer Science+Business Media New York},
