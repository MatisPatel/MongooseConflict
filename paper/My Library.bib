@article{brownEvolutionHeterogeneousEnvironments1992,
  title = {Evolution in Heterogeneous Environments: Effects of Migration on Habitat Specialization},
  shorttitle = {Evolution in Heterogeneous Environments},
  author = {Brown, Joel S. and Pavlovic, Noel B.},
  year = {1992},
  month = sep,
  journal = {Evolutionary Ecology},
  volume = {6},
  number = {5},
  pages = {360--382},
  issn = {0269-7653, 1573-8477},
  doi = {10.1007/BF02270698},
  abstract = {Richard Levins introduced fitness sets as a tool for investigating evolution within heterogeneous environments. Evolutionary game theory permits a synthesis and generalization of this approach by considering the evolutionary response of organisms to any scale of habitat heterogeneity. As scales of heterogeneity increase from fine to coarse, the evolutionary stable strategy (ESS) switches from a single generalist species to several species that become increasingly specialized on distinct habitats. Depending upon the organisms' ecology, the switch from one to two species may occur at high migration rates (relatively fine-grained environment), or may only occur at very low migration rates (coarse-grained environment). At the ESS, the evolutionary context of a species is the entire landscape, while its ecological context may be a single habitat. Evolution towards the ESS can be represented with adaptive landscapes. In the absence of frequency-dependence, shifting from a single strategy ESS to a two strategy ESS poses the problem of evolving across valleys in the adaptive surface to occupy new peaks (hence, Sewell Wright's shifting balance theory). Frequency-dependent processes facilitate evolution across valleys. If a system with a two strategy ESS is constrained to possess a single strategy, the population may actually evolve a strategy that minimizes fitness. Because the population now rests at the bottom of a valley, evolution by natural selection can drive populations to occupy both peaks.},
  langid = {english},
  keywords = {adaptive surface,ESS,evolution of specialization,Evolutionary Biology,fitness set,frequency-dependent selection,habitat selection,Human Genetics,landscape ecology,migration,Plant Sciences},
  file = {C\:\\Users\\defen\\Zotero\\storage\\K5CUCWXQ\\Brown and Pavlovic - 1992 - Evolution in heterogeneous environments Effects o.pdf;C\:\\Users\\defen\\Zotero\\storage\\AX5W56F9\\BF02270698.html}
}

@article{brownJointEvolutionMultiple2010,
  title = {Joint Evolution of Multiple Social Traits: A Kin Selection Analysis},
  shorttitle = {Joint Evolution of Multiple Social Traits},
  author = {Brown, Sam P. and Taylor, Peter D.},
  year = {2010},
  month = feb,
  journal = {Proceedings of the Royal Society of London B: Biological Sciences},
  volume = {277},
  number = {1680},
  pages = {415--422},
  issn = {0962-8452, 1471-2954},
  doi = {10.1098/rspb.2009.1480},
  abstract = {General models of the evolution of cooperation, altruism and other social behaviours have focused almost entirely on single traits, whereas it is clear that social traits commonly interact. We develop a general kin-selection framework for the evolution of social behaviours in multiple dimensions. We show that whenever there are interactions among social traits new behaviours can emerge that are not predicted by one-dimensional analyses. For example, a prohibitively costly cooperative trait can ultimately be favoured owing to initial evolution in other (cheaper) social traits that in turn change the cost\textendash benefit ratio of the original trait. To understand these behaviours, we use a two-dimensional stability criterion that can be viewed as an extension of Hamilton's rule. Our principal example is the social dilemma posed by, first, the construction and, second, the exploitation of a shared public good. We find that, contrary to the separate one-dimensional analyses, evolutionary feedback between the two traits can cause an increase in the equilibrium level of selfish exploitation with increasing relatedness, while both social (production plus exploitation) and asocial (neither) strategies can be locally stable. Our results demonstrate the importance of emergent stability properties of multidimensional social dilemmas, as one-dimensional stability in all component dimensions can conceal multidimensional instability.},
  copyright = {\textcopyright{} 2009 The Royal Society. This is an open-access article distributed under the terms of the Creative Commons Attribution License, which permits unrestricted use, distribution, and reproduction in any medium, provided the original work is properly cited.},
  langid = {english},
  pmid = {19828549},
  file = {C\:\\Users\\defen\\Zotero\\storage\\XXBKXP82\\Brown and Taylor - 2010 - Joint evolution of multiple social traits a kin s.pdf;C\:\\Users\\defen\\Zotero\\storage\\AIR2F298\\415.html}
}

